        },
        {
            "language": "javascript",
            "source": [
                "function birthdayCakeCandles(candles) {\n  \n    const maxHeight = Math.max(...candles);\n    \n   \n    let tallestCount = 0;\n    candles.forEach(height => {\n        if (height === maxHeight) {\n            tallestCount++;\n        }\n    });\n    \n \n    return tallestCount;\n}\n"
            ],
