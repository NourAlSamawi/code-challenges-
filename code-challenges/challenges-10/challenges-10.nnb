            "language": "markdown",
            "source": [
                "#####  [Migratory Birds](https://www.hackerrank.com/challenges/migratory-birds/problem?isFullScreen=true)\n\nSolution:"
            ],
            "outputs": []
        },
        {
            "language": "javascript",
            "source": [
                "function migratoryBirds(arr) {\n    const frequency = new Map();\n    for (const bird of arr) {\n        frequency.set(bird, (frequency.get(bird) || 0) + 1);\n    }\n    let maxFreq = 0, mostFrequentBird = 0;\n    frequency.forEach((count, bird) => {\n        if (count > maxFreq) {\n            maxFreq = count;\n            mostFrequentBird = bird;\n        } else if (count === maxFreq && bird < mostFrequentBird) {\n            mostFrequentBird = bird;\n        }\n    });\n    return mostFrequentBird;\n}\n"
            ],
            "outputs": []
        }
